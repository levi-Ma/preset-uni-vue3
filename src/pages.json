{
	"easycom": {
		"^u-(.*)": "./uni_modules/vk-uview-ui/components/u-$1/u-$1.vue"
	},
	"pages": [
		{
			"path": "pages/home",
			"style": {
				"navigationBarTitleText": "uViewUi"
			}
		},
		{
			"path": "pages/pinia",
			"style": {
				"navigationBarTitleText": "Pinia简单使用/hooks"
			}
		},
		{
			"path": "pages/poster",
			"style": {
				"navigationBarTitleText": "海报页"
			}
		}
	],
	"tabBar": {
		"color": "#4A4A4F",
		"selectedColor": "red",
		"backgroundColor": "#FFFFFF",
		"fontSize": "14px",
		"list": [
			{
				"iconPath": "./static/tabbar/home.png",
				"selectedIconPath": "./static/tabbar/home_1.png",
				"pagePath": "pages/home",
				"text": "首页"
			},
			{
				"iconPath": "./static/tabbar/message.png",
				"selectedIconPath": "./static/tabbar/message_1.png",
				"pagePath": "pages/poster",
				"text": "海报页"
			},
			{
				"iconPath": "./static/tabbar/pinia.png",
				"selectedIconPath": "./static/tabbar/pinia.png",
				"pagePath": "pages/pinia",
				"text": "其他"
			}
		]
	},
	"globalStyle": {
		"navigationBarTextStyle": "black",
		"navigationBarTitleText": "uni-app",
		"navigationBarBackgroundColor": "#F8F8F8",
		"backgroundColor": "#F8F8F8"
	},
	"subPackages": [
		{
			"root": "pakeage-poster",
			"pages": [
				{
					"path": "pages/index",
					"style": {
						"navigationBarTitleText": "海报页"
					}
				},
				{
					"path": "pages/qrcode",
					"style": {
						"navigationBarTitleText": "二维码"
					}
				}
			]
		},
		{
			"root": "pakeage-pinia",
			"pages": [
				{
					"path": "pages/index",
					"style": {
						"navigationBarTitleText": "Pinia简单使用"
					}
				},
				{
					"path": "pages/hook",
					"style": {
						"navigationBarTitleText": "Composables"
					}
				}
			]
		},
		{
			"root": "pakeage-button",
			"pages": [
				{
					"path": "pages/index",
					"style": {
						"navigationBarTitleText": "按钮组"
					}
				}
			]
		}
	]
}